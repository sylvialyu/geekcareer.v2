<div class="content">
<h1>Edit a Job</h1>

<%= simple_form_for @job do |f| %>

<%= f.input :title %>
<%= f.input :company_name %>
<%= f.input :description %>
<%= f.input :location %>
<%= f.input :published_time %>
<%= f.input :wage_lower_bound, :label => "薪资下限" %>
<%= f.input :wage_upper_bound, :label => "薪资上限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
<%= f.submit "Submit" %>

<% end %>
</div>
