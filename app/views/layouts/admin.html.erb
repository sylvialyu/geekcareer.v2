<!DOCTYPE html>
<html>
  <head>
    <title>geek career</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <div class="wrapper">
      <%= render "common/navbar" %>
      <%= render "common/flashes" %>
      <div class="content">
      <div class="row">
        <div class="col-md-2">
          <ul class="nav nav-pills nav-stacked" style="max-width: 10em; padding-top: 1.43em;">
            <li class="active"><%= link_to("Admin", admin_jobs_path) %></li>
            <li class="active"><%= link_to("Index", jobs_path) %></li>
            <li class="active"><%= link_to("Add a Job", new_admin_job_path) %></li>
          </ul>
        </div>
        <div class="col-md-10"><%= yield %></div>
      </div>
    </div>
      <div class="push"></div>
    </div>


    <%= render "common/footer"%>


  </body>
</html>
